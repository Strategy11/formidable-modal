{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "frm-modal/modal-2",
  "version": "0.1.0",
  "title": "Formidable Modal 2",
  "category": "widgets",
  "icon": "smiley",
  "description": "",
  "attributes": {
    "align": {
      "type": "string",
      "default": "center"
    },
    "backgroundColor": {
      "type": "string"
    },
    "style": {
      "type": "object"
    },
    "button_label": {
      "type": "string",
      "source": "html",
      "selector": ".frm_modal_button"
    },
    "button_type": {
      "type": "string"
    },
    "button_class": {
      "type": "string",
      "source": "attribute",
      "selector": ".frm_modal_button",
      "attribute": "class"
    },
    "modal_title": {
      "type": "string"
    },
    "modal_class": {
      "type": "string",
      "source": "attribute",
      "selector": ".frm_modal_wrapper",
      "attribute": "class"
    },
    "size": {
      "type": "string"
    }
  },
  "supports": {
    "html": false,
    "align": [ "left", "center", "right" ],
    "color": true,
    "defaultStylePicker": false
  },
  "textdomain": "frmmodal",
  "editorScript": "file:./index.js",
  "style": "file:./index.css"
}
